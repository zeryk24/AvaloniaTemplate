<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 xmlns:vm="using:AvaloniaTemplate.Presentation.Second"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="AvaloniaTemplate.Presentation.Second.SecondView"
			 x:DataType="vm:SecondViewModel">
	<Grid>
		<StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
			<TextBlock Text="You are on the second page!"/>
			<Button Content="Go back" HorizontalAlignment="Center" Command="{Binding GoBack}"/>
		</StackPanel>
	</Grid>
</UserControl>
